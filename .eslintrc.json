{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "no-unused-vars": "warn",
        "class-methods-use-this": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "ecmaFeatures": { "classes": true }
    }
}
